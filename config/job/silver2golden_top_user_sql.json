{
    "job_id": "silver2golden_top_user",
    "schedule": "",
    "depend_on": [
      "source2silver_customer",
      "source2silver_order"
    ],
    "run_mode": {
      "name": "default"
    },
    "extract": {
        "customers": {
            "table": "iceberg.silver.customer",
            "reader": "IcebergReader"
        },
        "orders": {
            "table": "iceberg.silver.order",
            "reader": "IcebergReader"
        }
    },
    "transform": {
      "transformer": "SQLTransformer",
      "transformation": "/opt/spark/sql-job/silver2golden_top_user.sql"
    },
    "load": {
        "result": {
            "table_name": "iceberg.golden.top_user_completed_order_in_day",
            "writer": "IcebergWriter",
            "mode": "overwrite_partition",
            "format": "parquet",
            "partition_by": [
                "partition_date"
            ],
            "path": "s3a://warehouse/silver/order"
        }
    }
}