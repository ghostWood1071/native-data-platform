                "driver": "oracle.jdbc.driver.OracleDriver"
            },
            "reader": "JDBCReader"
        }
    },
    "transform": {
        "transformer": "ConfigTransformer",
        "transformation": {
          "V_T_ERC_MONTHLY_DETAIL": [
            {
              "op": "withColumn",
              "args": {
                "colName": "partition_year",
                "col": "F.col('C_YEAR').cast(T.StringType())"
              }
            },
            {
              "op": "withColumn",
              "args": {
                "colName": "partition_month",
                "col": "F.col('C_MONTH').cast(T.StringType())"
              }
            },
            {
              "op": "withColumn",
              "args": {
                "colName": "etl_date",
                "col": "F.date_format(F.from_utc_timestamp(F.current_date(), 'Asia/Ho_Chi_Minh'), 'yyyy-MM-dd')"
              }
            }
          ]
        }
    },
    "load": {
        "V_T_ERC_MONTHLY_DETAIL": {
            "table_name": "spark_catalog.bronze.V_T_ERC_MONTHLY_DETAIL",
            "writer": "DeltaWriter",
            "mode": "overwrite",
            "first": true,
            "format": "delta",
            "partition_by": [
                "etl_date"
            ],
            "path": "s3a://warehouse/bronze/V_T_ERC_MONTHLY_DETAIL"
        }
    }
