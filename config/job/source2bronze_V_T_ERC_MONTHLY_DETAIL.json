            "table": "(SELECT * FROM MISUSER.V_T_ERC_MONTHLY_DETAIL WHERE C_YEAR = EXTRACT(YEAR FROM ADD_MONTHS(SYSDATE, -1)) AND C_MONTH = EXTRACT(MONTH FROM ADD_MONTHS(SYSDATE, -1))) tbl",
            "url": "jdbc:oracle:thin:@10.91.101.161:1521/tradingnkt",
            "properties": {
                "user": "mispoc",
